{
    "name": "dkulyk/eloquent-extensions",
    "description": "Eloquent extensions",
    "authors": [
        {
            "name": "Dmytro Kulyk",
            "email": "lnkvisitor.ts@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/database": "^5.8 | ^6.0 | ^7.0",
        "b2b/factory": "^1.0",
        "ramsey/uuid": "^3.0 | ^4.0"
    },
    "suggest": {
        "b2b/enum": "Use for enums casting."
    },
    "conflict": {
        "b2b/eloquent-extensions": "*"
    },
    "autoload": {
        "psr-4": {
            "DKulyk\\Eloquent\\Extensions\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DKulyk\\Eloquent\\Extensions\\ServiceProvider"
            ]
        }
    }
}
